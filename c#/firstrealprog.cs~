using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

class Program
{
	static void Main(string[] args)
	{
		const double a=5, b=7;
		double s=a*b/2;
		Console.WriteLine(s);
		double c=Math.Sqrt(Math.Pow(a,2)+Math.Pow(b,2));
		Console.WriteLine(c);
		double d=Math.Sqrt(Math.Pow(a,2)+Math.Pow(b,2)-2*a*b*Math.Cos(90*Math.PI/180));
		Console.WriteLine(d);
		double forRus=Math.Cos(90*Math.PI/180);
	}
}
		
