#include <iostream>
#include <cmath>
using namespace std;

char c;
int a=0, b=0;
void A();
void gc() { cin >> c; }

void S() {
	if (c == 'a') {
		a++;
		gc();		
		A();
		}
	else if (c == 'b') {
			b++;
			gc();
			A();
			}
		else
			throw c;
	}

